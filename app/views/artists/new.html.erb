<h1>New Artist:</h1>

<% if @errors%>
    <% @errors.each do |error|%>
        <p>Error: <%= error%></p>
    <% end%>
<% end%>


<%= form_for @artist do |f|%>
<%= f.label :name%>
<%= f.text_field :name%>
<%= f.label :title%>
<%= f.text_field :title%>
<%= f.label :age%>
<%= f.number_field :age%>

<%= f.submit%>
<%end%>